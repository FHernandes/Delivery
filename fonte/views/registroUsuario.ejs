	<main class="bg_gray">

		<div class="container margin_60_20">
			<div class="row justify-content-center">
				<div class="col-xl-6 col-lg-8">
					<div class="box_order_form">
						<div class="head">
							<div class="title">
								<h3>Dados Pessoais</h3>
							</div>
						</div>
						<!-- /head -->
						<div class="main">
							<div class="form-group">
								<label>Nome</label>
								<input class="form-control" placeholder="Nome Completo" id="nomeUsuario">
							</div>
							<div class="row">
								<div class="col-md-7">
									<div class="form-group">
										<label>Email</label>
										<input class="form-control" placeholder="exemplo@email.com" id="emailUsuario">
									</div>
								</div>
								<div class="col-md-5">
									<div class="form-group">
										<label>Telefone</label>
										<input class="form-control" placeholder="(XX) 99999-9999" id="telefoneUsuario">
									</div>
								</div>
							</div>
							<br>
							<div class="form-group">
								<label>Endereço</label>
								<input class="form-control" placeholder="Rua Exemplo" id="ruaUsuario">
							</div>
							<div class="row">
								<div class="col-md-6">
									<div class="form-group">
										<label>Bairro: </label>
										<input class="form-control" placeholder="Centro" id="bairroUsuario">
									</div>
								</div>
								<div class="col-md-6">
									<div class="form-group">
										<label>Número: </label>
										<input class="form-control" placeholder="123" id="numEndUsuario">
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-12">
									<div class="form-group">
										<label>Referência</label>
										<input class="form-control" placeholder="Em frente às quadras"
											id="refEndUsuario">
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-4">
									<div class="form-group">
									  <label>Cidade</label>
									  <input class="form-control" placeholder="Cidade" id="cidadeUsuario">
									</div>
								  </div>
								  <div class="col-md-4">
									<div class="form-group">
									  <label>UF</label>
									  <input class="form-control" placeholder="SP" id="estadoUsuario">
									</div>
								  </div>
								  <div class="col-md-4">
									<div class="form-group">
									  <label>CEP</label>
									  <input class="form-control" placeholder="00000-000" id="cepUsuario">
									</div>
								  </div>
							</div>
							<div class="row">
								<div class="col-md-12">
									<div class="form-group">										
										<button class="form-control btn-cadastrar" placeholder="Em frente às quadras" id="cadastroUsuário">Cadasdrar</button>
									</div>
								</div>
							</div>


						</div>
